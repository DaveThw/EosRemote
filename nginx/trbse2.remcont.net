# This server block will redirect from:
#  http://trbse2.remcont.net/... to https://trbse2.remcont.net/...
# and
#  http://192.168.2.11/...       to https://trbse2.remcont.net/...
server {
	listen 80;
	server_name trbse2.remcont.net 192.168.2.11;
	return 302 https://trbse2.remcont.net$request_uri;
}
